Pandoc
  Meta { unMeta = fromList [] }
  [ Div
      ( "" , [ "section" ] , [] )
      [ Header 2 ( "" , [] , [] ) [ Str "Poem title" ]
      , Div
          ( "" , [ "epigraph" ] , [] )
          [ Para [ Str "Poem epigraph" ] ]
      , Header 2 ( "" , [ "unnumbered" ] , [] ) [ Str "Subtitle" ]
      , Header 2 ( "" , [] , [] ) [ Str "First stanza title" ]
      , LineBlock
          [ [ Str "Verse" ] , [ Emph [ Str "More" ] , Str " verse" ] ]
      , LineBlock [ [ Str "One more stanza" ] ]
      , Para [ Str "Author" ]
      , Para [ Str "April 2018" ]
      ]
  ]
