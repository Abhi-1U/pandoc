Pandoc
  Meta
    { unMeta =
        fromList
          [ ( "author"
            , MetaList
                [ MetaInlines [ Str "Sam Smith" ]
                , MetaInlines [ Str "Jen Jones" ]
                ]
            )
          , ( "date" , MetaInlines [ Str "July 15, 2006" ] )
          , ( "title" , MetaInlines [ Str "My S5 Document" ] )
          ]
    }
  [ Header 1 ( "first-slide" , [] , [] ) [ Str "First slide" ]
  , BulletList
      [ [ Plain [ Str "first bullet" ] ]
      , [ Plain [ Str "second bullet" ] ]
      ]
  , Header 1 ( "math" , [] , [] ) [ Str "Math" ]
  , BulletList
      [ [ Plain
            [ Math
                InlineMath
                "\\frac{d}{dx}f(x)=\\lim_{h\\to 0}\\frac{f(x+h)-f(x)}{h}"
            ]
        ]
      ]
  ]
