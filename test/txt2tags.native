  , Header 1 ( "quote" , [] , [] ) [ Str "Quote" ]
  , BlockQuote
      [ Para
          [ Str "To quote a paragraph, just prefix it by a TAB"
          , SoftBreak
          , Str "character. All the lines of the paragraph must"
          , SoftBreak
          , Str "begin with a TAB."
          ]
      ]
  , Para [ Str "Any non-tabbed line closes the quote block." ]
  , BlockQuote
      [ Para
          [ Str "The number of leading TABs identifies the quote"
          , SoftBreak
          , Str "block depth. This is quote level 1."
          ]
      , BlockQuote
          [ Para
              [ Str "With two TABs, we are on the quote"
              , SoftBreak
              , Str "level 2."
              ]
          , BlockQuote
              [ Para
                  [ Str "The more TABs, more deep is"
                  , SoftBreak
                  , Str "the quote level."
                  ]
              , BlockQuote [ Para [ Str "There isn't a limit." ] ]
              ]
          ]
      ]
  , BlockQuote
      [ BlockQuote
          [ BlockQuote
              [ BlockQuote
                  [ Para
                      [ Str "This quote starts at"
                      , SoftBreak
                      , Str "level 4."
                      ]
                  ]
              , Para [ Str "Then its depth is decreased." ]
              ]
          , Para [ Str "Counting down, one by one." ]
          ]
      , Para [ Str "Until the level 1." ]
      ]
  , BlockQuote
      [ BlockQuote
          [ BlockQuote
              [ Para
                  [ Str "Unlike lists, any quote block is"
                  , SoftBreak
                  , Str "independent, not part of a tree."
                  ]
              ]
          ]
      , Para
          [ Str "The TAB count don't need to be incremental"
          , SoftBreak
          , Str "by one."
          ]
      , BlockQuote
          [ BlockQuote
              [ BlockQuote
                  [ Para
                      [ Str "The nesting don't need"
                      , SoftBreak
                      , Str "to follow any rule."
                      ]
                  ]
              ]
          , Para
              [ Str "Quotes can be opened and closed"
              , SoftBreak
              , Str "in any way."
              ]
          , BlockQuote
              [ BlockQuote [ BlockQuote [ Para [ Str "You choose." ] ] ]
              ]
          ]
      ]
  , BlockQuote
      [ Para
          [ Str "Some targets (as sgml) don't support the"
          , SoftBreak
          , Str "nesting of quotes. There is only one quote"
          , SoftBreak
          , Str "level."
          ]
      , BlockQuote
          [ Para
              [ Str "In this case, no matter how much"
              , SoftBreak
              , Str "TABs are used to define the quote"
              , SoftBreak
              , Str "block, it always will be level 1."
              ]
          ]
      ]
  , BlockQuote
      [ Para
          [ Str "Spaces AFTER the TAB character are allowed."
          , SoftBreak
          , Str "But be careful, it can be confusing."
          ]
      ]
  , Para
      [ Str "Spaces BEFORE the TAB character"
      , SoftBreak
      , Str "invalidate the mark. It's not quote."
      ]
  , BlockQuote
      [ Para
          [ Str "Paragraph breaks inside a quote aren't"
          , SoftBreak
          , Str "possible."
          ]
      , Para
          [ Str "This sample are two separated quoted"
          , SoftBreak
          , Str "paragraphs, not a quote block with"
          , SoftBreak
          , Str "two paragraphs inside."
          ]
      ]
  , BlockQuote
      [ Para
          [ Str "The end of the file (EOF) closes the"
          , SoftBreak
          , Str "currently open quote block."
          ]
      ]
