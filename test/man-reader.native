  , Header 1 ( "" , [] , [] ) [ Str "Paragraphs" ]
  , Para [ Str "Here's a regular paragraph." ]
  , Para
      [ Str
          "Another paragraph In Markdown 1.0.0 and earlier. Version 8. This line turns into a list item. Because a hard-wrapped line in the middle of a paragraph looked like a list item."
      ]
  , Para [ Str "There should be a hard line break" ]
  , Para [ Str "here." ]
  , Para [ Str "* * * * *" ]
  , Header 1 ( "" , [] , [] ) [ Str "Block Quotes" ]
