Pandoc
  Meta { unMeta = fromList [] }
  [ Para
      [ Str "Here are examples of "
      , Emph [ Str "italics" ]
      , Str ", "
      , Strong [ Str "bold" ]
      , Str ", and "
      , Strong [ Emph [ Str "bold italics" ] ]
      , Str "."
      ]
  , Para
      [ Str "Here is "
      , Strikeout [ Str "strook-three" ]
      , Str " strike-through and "
      , SmallCaps [ Str "small caps" ]
      , Str "."
      ]
  , Para
      [ Str "Here is "
      , Span
          ( "" , [ "underline" ] , [] )
          [ Str "some "
          , Emph [ Str "underlined" ]
          , Str " "
          , Strong [ Str "text" ]
          ]
      , Str "."
      ]
  , Para
      [ Str "We can also do subscripts (H"
      , Subscript [ Str "2" ]
      , Str "0) and super"
      , Superscript [ Str "script" ]
      , Str "."
      ]
  , RawBlock
      (Format "html") "<!-- Comments don't show up. -->"
  ]
