Pandoc
  Meta
    { unMeta =
        fromList
          [ ( "Company" , MetaInlines [ Str "My Company" ] )
          , ( "Second Custom Property"
            , MetaInlines [ Str "Second custom property value" ]
            )
          , ( "abstract"
            , MetaBlocks
                [ Plain
                    [ Str "Quite a long description"
                    , SoftBreak
                    , Str "spanning several lines"
                    ]
                ]
            )
          , ( "author" , MetaList [ MetaInlines [ Str "A. M." ] ] )
          , ( "category" , MetaInlines [ Str "My Category" ] )
          , ( "custom1"
            , MetaInlines [ Str "First custom property value" ]
            )
          , ( "custom3" , MetaInlines [ Str "Escaping amp & ." ] )
          , ( "custom4"
            , MetaInlines [ Str "Escaping LT,GT < asdf > <" ]
            )
          , ( "custom5"
            , MetaInlines
                [ Str "Escaping html "
                , RawInline (Format "html") "<i>"
                , Str "asdf"
                , RawInline (Format "html") "</i>"
                ]
            )
          , ( "custom6"
            , MetaInlines
                [ Str "Escaping " , Emph [ Str "MD" ] , Str " \225 a" ]
            )
          , ( "custom9"
            , MetaInlines
                [ Str "Extended chars: \8364 \225 \233 \237 \243 \250 $"
                ]
            )
          , ( "description"
            , MetaBlocks
                [ Para
                    [ Str "Long description spanning"
                    , SoftBreak
                    , Str "several lines."
                    ]
                , Plain
                    [ Str "This is \225 second "
                    , RawInline (Format "html") "<i>"
                    , Str "line"
                    , RawInline (Format "html") "</i>"
                    , Str "."
                    ]
                ]
            )
          , ( "keywords"
            , MetaList
                [ MetaInlines [ Str "keyword 1" ]
                , MetaInlines [ Str "keyword 2" ]
                ]
            )
          , ( "lang" , MetaInlines [ Str "en-US" ] )
          , ( "nested-custom"
            , MetaList
                [ MetaMap
                    (fromList
                       [ ( "custom 7"
                         , MetaInlines [ Str "Nested Custom value 7" ]
                         )
                       ])
                , MetaMap
                    (fromList
                       [ ( "custom 8"
                         , MetaInlines [ Str "Nested Custom value 8" ]
                         )
                       ])
                ]
            )
          , ( "subject" , MetaInlines [ Str "This is the subject" ] )
          , ( "subtitle" , MetaInlines [ Str "This is a subtitle" ] )
          , ( "title"
            , MetaInlines [ Str "Testing custom properties" ]
            )
          ]
    }
  [ Para [ Str "Testing document properties" ] ]
