Pandoc
  Meta
    { unMeta =
        fromList
          [ ( "author" , MetaInlines [ Str "Cynthia Johnson" ] )
          , ( "operator" , MetaInlines [ Str "John MacFarlane" ] )
          , ( "title" , MetaInlines [ Str "Text before list" ] )
          ]
    }
  [ OrderedList
      ( 1 , Decimal , Period )
      [ [ Para [ Str "One" ] ]
      , [ Para [ Str "Two" ]
        , OrderedList
            ( 1 , LowerAlpha , Period )
            [ [ Para [ Str "Three" ] ]
            , [ Para [ Str "Four" ]
              , OrderedList
                  ( 1 , LowerRoman , Period )
                  [ [ Para [ Str "Five" ] ]
                  , [ Para [ Str "Six" ]
                    , OrderedList
                        ( 1 , UpperAlpha , Period )
                        [ [ Para [ Str "Seven" ] ]
                        , [ Para [ Str "Eight" ]
                          , OrderedList
                              ( 1 , UpperRoman , Period )
                              [ [ Para [ Str "Nine" ] ]
                              , [ Para [ Str "Ten" ]
                                , BulletList
                                    [ [ Para [ Str "Eleven" ] ]
                                    , [ Para [ Str "Twelve" ] ]
                                    ]
                                ]
                              ]
                          ]
                        ]
                    ]
                  ]
              ]
            ]
        ]
      ]
  , Para [ Str "Out of list!" ]
  , Para [ Str "Start with" ]
  , OrderedList
      ( 7 , Decimal , Period )
      [ [ Para [ Str "Seven Start" ] ]
      , [ Para [ Str "Eight Continue" ] ]
      ]
  ]
