Pandoc
  Meta { unMeta = fromList [] }
  [ Para [ Str "This is a simple text" ]
  , Para []
  , Para
      [ Str "that is both "
      , Emph [ Str "italic " ]
      , Strong [ Str "bold " ]
      , Emph [ Str "underlined " ]
      , Str "and the "
      , Emph [ Strong [ Str "first two" ] ]
      , Str " and the "
      , Emph [ Strong [ Str "last two " ] ]
      , Str " and "
      , Strong [ Strikeout [ Str "bold and line through" ] ]
      ]
  , Para []
  , Para
      [ Str "And with "
      , Superscript [ Emph [ Str "superscripts" ] ]
      ]
  ]
