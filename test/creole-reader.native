      ]
  , Para [ Str "unnumbered lists are like" ]
  , BulletList
      [ [ Plain [ Str "item a" ] ]
      , [ Plain [ Str "item b" ] ]
      , [ Plain [ Strong [ Str "bold item c" ] ] ]
      ]
  , Para
      [ Str "blank space is also permitted before lists like:" ]
  , BulletList
      [ [ Plain [ Str "item a" ] ]
      , [ Plain [ Str "item b" ] ]
      , [ Plain [ Str "item c" ]
        , BulletList [ [ Plain [ Str "item c.a" ] ] ]
        ]
      ]
  , Para [ Str "or you can number them" ]
  , OrderedList
      ( 1 , DefaultStyle , DefaultDelim )
      [ [ Plain
            [ Link ( "" , [] , [] ) [ Str "item 1" ] ( "item 1" , "" ) ]
        ]
      , [ Plain [ Str "item 2" ] ]
      , [ Plain [ Emph [ Str " italic item 3 " ] ]
        , OrderedList
            ( 1 , DefaultStyle , DefaultDelim )
            [ [ Plain [ Str "item 3.1" ] ]
            , [ Plain [ Str "item 3.2" ] ]
            ]
        ]
      ]
