      , Str " or "
      , Emph [ Strong [ Str "both" ] ]
      , Str "."
      ]
  , Para
      [ Str "Character formatting extends across line breaks: "
      , Strong
          [ Str
              "bold, this is still bold. This line deliberately does not end in star-star."
          ]
      ]
  , Para
      [ Str
          "Not bold. Character formatting does not cross paragraph boundaries."
      ]
  , Para
