Pandoc
  Meta { unMeta = fromList [] }
  [ Div
      ( "" , [] , [ ( "custom-style" , "First Paragraph" ) ] )
      [ Para [ Str "This is some text." ] ]
  , Div
      ( "" , [] , [ ( "custom-style" , "Body Text" ) ] )
      [ Para
          [ Str "This is text with an "
          , Span
              ( "" , [] , [ ( "custom-style" , "Emphatic" ) ] )
              [ Str "emphasized" ]
          , Str " text style. And this is text with a "
          , Span
              ( "" , [] , [ ( "custom-style" , "Strengthened" ) ] )
              [ Str "strengthened" ]
          , Str " text style."
          ]
      ]
  , Div
      ( "" , [] , [ ( "custom-style" , "My Block Style" ) ] )
      [ BlockQuote
          [ Para
              [ Str
                  "Here is a styled paragraph that inherits from Block Text."
              ]
          ]
      ]
  ]
