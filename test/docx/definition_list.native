Pandoc
  Meta { unMeta = fromList [] }
  [ DefinitionList
      [ ( [ Str "Term 1" ] , [ [ Para [ Str "Definition 1" ] ] ] )
      , ( [ Str "Term 2 with " , Emph [ Str "inline markup" ] ]
        , [ [ Para [ Str "Definition 2" ]
            , CodeBlock
                ( "" , [] , [] ) "{ some code, part of Definition 2 }"
            , Para [ Str "Third paragraph of definition 2." ]
            ]
          ]
        )
      ]
  ]
