Pandoc
  Meta { unMeta = fromList [] }
  [ Para [ Str "This is a test of custom-styles." ]
  , Para
      [ Str "Here is something "
      , Span
          ( "" , [] , [ ( "custom-style" , "Emphatic" ) ] )
          [ Str "emphasized" ]
      , Str ". And"
      , SoftBreak
      , Str "here is something "
      , Span
          ( "" , [] , [ ( "custom-style" , "Strengthened" ) ] )
          [ Str "strong" ]
      , Str "."
      ]
  , Div
      ( "" , [] , [ ( "custom-style" , "My Block Style" ) ] )
      [ Para [ Str "One paragraph of text." ]
      , Para
          [ Str "And another paragraph of "
          , Span
              ( "" , [] , [ ( "custom-style" , "Emphatic" ) ] )
              [ Str "really" , SoftBreak , Str "cool" ]
          , Str " text."
          ]
      ]
  ]
