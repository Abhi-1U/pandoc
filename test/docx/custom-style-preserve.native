Pandoc
  Meta { unMeta = fromList [] }
  [ Para
      [ Span
          ( "" , [] , [ ( "custom-style" , "MyStyle" ) ] )
          [ Str "This span"
          , Note
              [ Para
                  [ Str
                      "Neither footnote nor footnote reference should get a custom style from its span."
                  ]
              ]
          , Str " should have a custom style ("
          , Link
              ( "" , [] , [] )
              [ Str "link" ]
              ( "http://example.com/" , "" )
          , Str "),"
          ]
      , Str
          " but the text after the comma shouldn\8217t, nor should the link."
      ]
  , Div
      ( "" , [] , [ ( "custom-style" , "MyOtherStyle" ) ] )
      [ Para
          [ Str
              "The contents of this div should have a custom style, but "
          , Link
              ( "" , [] , [] )
              [ Str "this link should not" ]
              ( "http://example.com/" , "" )
          , Str "."
          ]
      , Header
          2
          ( "this-header-should-not-have-the-divs-custom-style"
          , []
          , []
          )
          [ Str
              "This header should not have the div\8217s custom style"
          ]
      , BlockQuote [ Para [ Str "This blockquote should not." ] ]
      , CodeBlock ( "" , [] , [] ) "# This code block should not."
      , Para
          [ Str "But this paragraph should."
          , Note
              [ Para
                  [ Str
                      "Neither footnote nor footnote reference should get a custom style from its div."
                  ]
              ]
          ]
      ]
  , Div
      ( "" , [] , [ ( "custom-style" , "MyOuterStyle" ) ] )
      [ Div
          ( "" , [] , [ ( "custom-style" , "MyInnerStyle" ) ] )
          [ Para [ Str "This should have MyInnerStyle." ]
          , Header
              3
              ( "this-heading-should-not" , [] , [] )
              [ Str "This heading should not" ]
          ]
      , Para
          [ Str
              "This should have MyOuterStyle, but the following elision should have its"
          , SoftBreak
          , Str "own style. "
          , Span
              ( "" , [] , [ ( "custom-style" , "Elision" ) ] )
              [ Str "..." ]
          ]
      , BlockQuote
          [ Para
              [ Str "This blockquote should include "
              , Strong
                  [ Str "bold text with an elision:"
                  , SoftBreak
                  , Span
                      ( "" , [] , [ ( "custom-style" , "Elision" ) ] )
                      [ Str "..." ]
                  ]
              ]
          ]
      ]
  ]
