      , Strong [ Str "schedule" ]
      , Str " and contacts where available:"
      ]
  , Para [ Str "\160" ]
  , OrderedList
      ( 1 , Decimal , Period )
      [ [ BlockQuote [ Para [ Str "State of Documentation" ] ] ] ]
  , BlockQuote
      [ Para
          [ Strong
              [ Str
                  "Goal: Baseline and ongoing metrics tracking doc usefulness and completeness."
              ]
          ]
      , Para [ Str "\160" ]
      ]
  , OrderedList
      ( 2 , Decimal , Period )
      [ [ BlockQuote [ Para [ Str "Content Migration" ] ] ] ]
