      [ Para
          [ Str
              "Goal: Content is accessible to new employees and is better organized/archived."
          ]
      , Para [ Str "\160" ]
      ]
  , OrderedList
      ( 3 , Decimal , Period )
      [ [ BlockQuote [ Para [ Str "Wiki (xl)" ] ] ] ]
  , BlockQuote
      [ Para
          [ Strong
              [ Str
                  "Goal: Useful documentation that is archived, searchable and easy to create"
              ]
          ]
      , Para [ Str "\160\160" ]
      ]
  , OrderedList
      ( 4 , Decimal , Period )
      [ [ BlockQuote
            [ Para [ Str "XL Code Autoreview Bot (XLCRBot)." ] ]
        ]
      ]
