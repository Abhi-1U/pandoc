      ( 5 , Decimal , Period )
      [ [ BlockQuote [ Para [ Str "Code documentation" ] ] ] ]
  , BlockQuote
      [ Para
          [ Strong
              [ Str "Goal: Useful, consistent, tool supported comments"
              ]
          ]
      ]
  , Para [ Strong [ Str "\160" ] , Str "\160" ]
  , OrderedList
      ( 6 , Decimal , Period )
      [ [ BlockQuote [ Para [ Str "Education efforts" ] ] ] ]
  , BlockQuote
      [ Para
          [ Strong
              [ Str
                  "Goal: Broad, discoverable channels for updates and news"
              ]
          ]
      ]
  , Para [ Strong [ Str "\160" ] ]
